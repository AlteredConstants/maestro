{
  "name": "maestro-server",
  "version": "0.0.1",
  "description": "Server backend for Maestro fencing tournament management suite.",
  "private": true,
  "main": "lib/entry.js",
  "dependencies": {
    "bluebird": "^3.4.6",
    "camo": "^0.12.3",
    "check-types": "^7.0.1",
    "es6-promisify": "^5.0.0",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^3.2.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.0.1",
    "lodash": "^4.17.2",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.0",
    "source-map-support": "^0.4.6",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-env": "0.0.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "deep-freeze": "0.0.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-react": "^6.7.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "mocha-silent-reporter": "^1.0.0",
    "mochawesome": "^1.5.4",
    "nodemon": "^1.11.0",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.5.4",
    "testdouble": "^1.9.0",
    "testdouble-chai": "^0.4.0",
    "wait-on": "^2.0.1"
  },
  "scripts": {
    "start": "node .",
    "start:dev": "concurrently \"npm run watch\" \"npm run serve:delay\" --kill-others --prefix \"[{name}]\" --names \"BUILD,SERVE\"",
    "build": "rimraf lib && babel src --out-dir lib --source-maps",
    "watch": "npm run build -- --watch",
    "serve": "nodemon --watch lib --debug=5858 lib/entry.js",
    "serve:delay": "wait-on lib/entry.js --delay 700 && npm run serve",
    "lint": "eslint .",
    "test": "istanbul cover node_modules/mocha/bin/_mocha --",
    "test:watch": "npm run test -- --watch",
    "pretest:report": "npm run test -- --reporter mochawesome --reporter-options reportDir=.temp/test-report || exit 0",
    "test:report": "opn .temp/coverage/lcov-report/index.html && opn .temp/test-report/mochawesome.html",
    "verify": "npm run lint && npm run test -- --reporter mocha-silent-reporter"
  },
  "author": "Zeff Svoboda",
  "license": "ISC"
}
